{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--no-deprecation' vite",
    "build": "NODE_OPTIONS='--no-deprecation' vite build",
    "preview": "NODE_OPTIONS='--no-deprecation' vite preview",
    "test": "NODE_OPTIONS='--no-deprecation' jest --config jest.config.cjs",
    "test:coverage": "NODE_OPTIONS='--no-deprecation' jest --config jest.config.cjs --coverage; node scripts/coverage-summary.cjs",
    "test:e2e": "NODE_OPTIONS='--no-deprecation' playwright test",
    "e2e": "NODE_OPTIONS='--no-deprecation' playwright test",
    "e2e:ui": "NODE_OPTIONS='--no-deprecation' playwright test --ui",
    "e2e:headed": "NODE_OPTIONS='--no-deprecation' playwright test --headed",
    "e2e:report": "playwright show-report playwright-report --host 127.0.0.1 --port 9324",
    "e2e:desktop": "NODE_OPTIONS='--no-deprecation' playwright test --project=\"Desktop Chrome\"",
    "e2e:mobile": "NODE_OPTIONS='--no-deprecation' playwright test --project=\"Mobile Chrome\"",
    "e2e:tablet": "NODE_OPTIONS='--no-deprecation' playwright test --project=\"Tablet\"",
    "e2e:coverage": "npm run e2e:clean && NODE_OPTIONS='--no-deprecation' playwright test",
    "e2e:coverage:ci": "npm run e2e:clean && NODE_OPTIONS='--no-deprecation' playwright test --reporter=./e2e/reporters/flow-coverage-reporter.mjs",
    "e2e:coverage:module": "npm run e2e:clean && node ./scripts/e2e-coverage-module.cjs",
    "e2e:module": "npm run e2e:clean && node ./scripts/e2e-module.cjs",
    "e2e:modules": "npm run e2e:clean && node ./scripts/e2e-modules.cjs",
    "e2e:full": "npm run e2e:clean && npm run e2e:coverage",
    "e2e:clean": "rm -rf test-results playwright-report e2e-results"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "axios": "^1.13.5",
    "bootstrap-icons": "^1.13.1",
    "flowbite": "^4.0.1",
    "gsap": "^3.14.2",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "sweetalert2": "^11.26.20",
    "vue": "^3.5.29",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.3",
    "vue3-google-login": "^2.0.37"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.24",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.39.3",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.1",
    "v8-to-istanbul": "^9.2.0",
    "vite": "^6.4.1"
  }
}
